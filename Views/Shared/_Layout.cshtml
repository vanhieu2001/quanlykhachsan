<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - qlks</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/qlks.styles.css" asp-append-version="true" />
    <link href="https://fonts.googleapis.com/css?family=Lato:300,400,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Kaushan+Script" rel="stylesheet">

    <!-- Animate.css -->
    <link rel="stylesheet" href="~/Content/Home/css/animate.css">
    <!-- Icomoon Icon Fonts-->
    <link rel="stylesheet" href="~/Content/Home/css/icomoon.css">
    <!-- Themify Icons-->
    <link rel="stylesheet" href="~/Content/Home/css/themify-icons.css">
    <!-- Bootstrap  -->
    <link rel="stylesheet" href="~/Content/Home/css/bootstrap.css">

    <!-- Magnific Popup -->
    <link rel="stylesheet" href="~/Content/Home/css/magnific-popup.css">

    <!-- Bootstrap DateTimePicker -->
    <link rel="stylesheet" href="~/Content/Home/css/bootstrap-datetimepicker.min.css">

    <!-- Owl Carousel  -->
    <link rel="stylesheet" href="~/Content/Home/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Content/Home/css/owl.theme.default.min.css">

    <link href="~/Content/Admin/vendor/datatables/dataTables.bootstrap4.css" rel="stylesheet">

    <!-- Theme style  -->
    <link rel="stylesheet" href="~/Content/Home/css/style.css">
    <link rel="stylesheet" href="~/Content/Site.css">
    <!-- Modernizr JS -->
    <script src="~/Content/Home/js/modernizr-2.6.2.min.js"></script>
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">qlks</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - qlks - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <!-- jQuery -->
    <script src="~/Content/Home/js/jquery.min.js"></script>
    <!-- jQuery Easing -->
    <script src="~/Content/Home/js/jquery.easing.1.3.js"></script>
    <!-- Bootstrap -->
    <script src="~/Content/Home/js/bootstrap.min.js"></script>
    <!-- Waypoints -->
    <script src="~/Content/Home/js/jquery.waypoints.min.js"></script>
    <!-- Carousel -->
    <script src="~/Content/Home/js/owl.carousel.min.js"></script>
    <!-- countTo -->
    <script src="~/Content/Home/js/jquery.countTo.js"></script>

    <!-- Stellar Parallax -->
    <script src="~/Content/Home/js/jquery.stellar.min.js"></script>

    <!-- Magnific Popup -->
    <script src="~/Content/Home/js/jquery.magnific-popup.min.js"></script>
    <script src="~/Content/Home/js/magnific-popup-options.js"></script>

    <script src="~/Content/Home/js/moment.min.js"></script>
    <script src="~/Content/Home/js/bootstrap-datetimepicker.min.js"></script>


    <!-- Main -->
    <script src="~/Content/Home/js/main.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
